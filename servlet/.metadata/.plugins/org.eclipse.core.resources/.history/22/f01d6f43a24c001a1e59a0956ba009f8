package com.tyss.inventorymanagement.dao;

import java.util.List;

import com.tyss.inventorymanagement.dto.ProductInfo;
import com.tyss.productspringboot.dto.ProductInfoBean;

public interface InventoryManagement {
	List<ProductInfoBean> getProductByName(String name);
	void addProduct(ProductInfo productInfo);
	boolean updateProduct(ProductInfo productInfo);
	boolean removeProduct(int id);
	List<ProductInfo> getAllProduct();

}
